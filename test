pipeline {
    agent any
    parameters {
        string(name: 'Greeting', defaultValue: 'Hello');
        string(name: 'Ending', defaultValue: 'Good bye');
        string(name: 'Username', defaultValue: 'HuongPhan')
        }
    
    stages {
        stage('Hello') {
            steps {
                echo 'Hello world!'
                echo "Running ${env.BUILD_DISPLAY_NAME} on  ${env.BUILD_NUMBER} with ${env.BRANCH_NAME}"
                }
            }
            
        stage('Build 1') {
            steps {
                build 'HuongPhan'
                }
            } 
            
        stage('Build 2') {
            steps {
                build 'j1'
                }
            }  
        stage('Build 3') {
            steps {
                build 'test1'
                }
            } 
            
        stage('The end') {
            steps {
                echo "Goodbye ${params.Username}!"
                }
            }    
        }
    }   
